<!DOCTYPE html>
    <head>
        <title> Employee Update</title>
    </head>

    <body>
        <div>
            
            <form method="POST" action="/employeeUpdate?_method=PUT">
                <h1> Updating Employee </h1>
                <label for="employeeIdU">Employee Id: </label>
                <input type="text" name="employeeIdU" value=<%= employeeId %>>
                <br>

                <label for="employeeGmailU">Employee Gmail: </label>
                <input type="text" name="employeeGmailU" value=<%= employeeGmail %>>
                <br>

                <label for="employeeNameU">Employee Name: </label>
                <input type="text" name="employeeNameU" value=<%= employeeName %>>
                <br>

                <label for="employeeAgeU">Employee Age: </label>
                <input type="text" name="employeeAgeU" value=<%= employeeAge %>>
                <br>

                <label for="employeeBirthDayU">Employee Birthday: </label>
                <input type="text" name="employeeBirthDayU" value=<%= employeeBirthDay %>>
                <br>
                
                <button type="submit">Submit</button>
            </form>
            
        </div>
    </body>
</html>